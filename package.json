{
  "name": "beidoums-scripts-snippets",
  "displayName": "beidoums-scripts-snippets",
  "description": "some scripts snippets and code completion for beidoums",
  "publisher": "shinobi9",
  "author": "shinobi9",
  "version": "0.0.4",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/beidoums-stateless.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/beidoums-vanilla.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/shinobi9-personal.code-snippets"
      }
    ],
    "typescriptServerPlugins": [
      {
        "name": "beidoums-scripts-types",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinobi9/beidoums-scripts-snippets"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js"
}