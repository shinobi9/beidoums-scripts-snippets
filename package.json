{
  "name": "beidoums-scripts-snippets",
  "displayName": "beidoums-scripts-snippets",
  "description": "some scripts snippets and code completion for beidoums",
  "publisher": "shinobi9",
  "author": "shinobi9",
  "version": "0.0.5",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinobi9/beidoums-scripts-snippets"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Snippets", "Other"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/beidoums-stateless.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/beidoums-vanilla.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/shinobi9-personal.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "beidoums-scripts-snippets.create-jsconfig",
        "title": "create beidoums jsconfig.json",
        "category": "Beidoums Scripts Snippets"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:beidoums-scripts-snippets.create-jsconfig"
  ],
  "main": "./extension.js"
}